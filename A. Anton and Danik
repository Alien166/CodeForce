#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n;
    char ch;
    while (cin>>n) {
        int a = 0, d = 0;
        for (int i = 0; i < n; i++) {
            cin >> ch;
            if (ch == 'A') {
                a++;
            } else {
                d++;
            }
        }

        if (a > d) {
            cout << "Anton\n";
        } else if (d > a) {
            cout << "Danik";
        } else
            cout << "Friendship";
    }
    return 0;

}
