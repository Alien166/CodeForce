#include<bits/stdc++.h>
using namespace std;
int main()
{
    string str;
    cin>>str;
    int c=1;
   for(int i=1;i<str.length();++i)
   {
       if(str[i]==str[i-1])
       {
           c+=1;
           if(c==7)
           {
               cout<<"YES";
               return 0;
           }
       }
       else
       {
           c = 1;
       }
   }
   cout<<"NO";
    return 0;
}
