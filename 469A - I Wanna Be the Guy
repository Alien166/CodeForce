#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
    int n,p,q;
    cin>>n>>p;
    int arr[2005];
    int ans=0;
    for(int i=0;i<p;i++){
        cin>>arr[i];
    }
    cin>>q;
    for(int i=p;i<p+q;i++)
    {
        cin>>arr[i];
    }
    sort(arr,arr+(p+q));
    for (int i = 0; i <p+q; i++) {
        if(arr[i]!=arr[i+1])
        {
            ans++;
        }
    }
    if(n==(ans))
    {
        cout<<"I become the guy."<<endl;
    } else{
        cout<<"Oh, my keyboard!"<<endl;
    }
    return 0;
}
