#include<bits/stdc++.h>
using namespace std;
#define ll long long
int main()
{
    int n,m,ans=0;
    cin>>n>>m;
    char arr[n][m];
    for(int i=0;i<n;++i)
    {
        for (int j = 0; j < m; ++j)
        {
            cin >> arr[i][j];
            if(arr[i][j]=='C'||arr[i][j]=='M'||arr[i][j]=='Y')
            {
                ans++;
            }

        }
    }
    if(ans>0){
        cout<<"#Color"<<endl;
    }
    else {
        cout << "#Black&White" << endl;
    }

    return 0;
}
