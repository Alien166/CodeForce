#include<bits/stdc++.h>
using namespace std;
int main()
{
    int n=4,ans=0;
    int arr[n];
    for(int i=0;i<n;++i)
    {
        cin>>arr[i];
    }
    sort(arr,arr+n);
    for(int i=0;i<n;++i)
    {
        if (arr[i] != arr[i + 1]) {
            ans++;
        }
    }
    cout<<n-ans;
    return 0;

}
